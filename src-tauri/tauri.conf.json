{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://127.0.0.1:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "jelly-downloader",
    "version": "0.0.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.therolf.jellydonwloader",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "jelly-downloader",
        "width": 800,
        "height": 600
      }
    ]
  },
  "$schema": "https://raw.githubusercontent.com/tauri-apps/tauri/tauri-v2.0.0-alpha.21/core/tauri-config-schema/schema.json"
}
